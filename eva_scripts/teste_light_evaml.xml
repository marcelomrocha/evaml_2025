<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="teste" xsi:noNamespaceSchemaLocation="../robot_package/xml_schema/evaml_schema.xsd">
  <settings>
    <voice type="en-US_EmmaExpressive" />
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <light state="OFF" />
    <led animation="STOP" />
    <evaEmotion emotion="NEUTRAL" />
    <wait duration="2000" />
    <light state="ON" />
    <wait duration="2000" />
    <led animation="SPEAK" />
    <wait duration="2000" />
    <evaEmotion emotion="HAPPY" />
    <audio source="efx-robot-head" block="TRUE" />
    <light state="OFF" />
    <led animation="STOP" />
    <evaEmotion emotion="NEUTRAL" />
  </script>
</evaml>