<?xml version="1.0" encoding="UTF-8"?>
<evaml
  name="teste"
  xsi:noNamespaceSchemaLocation="../robot_package/xml_schema/evaml_schema.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <settings>
    <voice type="en-US_EmmaExpressive" />
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <led animation="STOP" />
    <evaEmotion emotion="NEUTRAL" />
    <wait duration="2000" />
    <evaEmotion emotion="HAPPY" />
    <led animation="HAPPY" />
    <wait duration="5000" />
    <evaEmotion emotion="ANGRY" />
    <led animation="ANGRY" />
    <wait duration="5000" />
    <motion head="2RIGHT"/>
    <wait duration="4000" />
    <led animation="STOP" />
  </script>
</evaml>