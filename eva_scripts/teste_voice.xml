<?xml version="1.0" encoding="UTF-8"?>
<evaml
  name="teste"
  xsi:noNamespaceSchemaLocation="../robot_package/xml_schema/evaml_schema.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <settings>
    <voice type="pt_BR-faber-medium" pitchShift="0"/>
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <talk voiceType="pt_BR-faber-medium">Olá, meu nome é Frédi! Como você se chama?</talk>
    <listen />
    <talk voiceType="pt_BR-faber-medium">Muito prazer em conhecê-lo, $</talk>
    <audio source="efx-display" block="TRUE" />
    <talk voiceType="pt_BR-faber-medium" pitchShift="8">Olá, meu nome é Frédi! Como você se chama?</talk>
    <listen />
    <talk voiceType="pt_BR-faber-medium" pitchShift="8">Muito prazer em conhecê-lo, $</talk>
    <audio source="efx-display" block="TRUE" />
    <talk voiceType="en_US-amy-medium" pitchShift="5">Hi, my name is Fréd! How are you?</talk>
  </script>
</evaml>