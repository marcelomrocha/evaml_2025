<?xml version="1.0" encoding="UTF-8"?>
<evaml
  name="teste"
  xsi:noNamespaceSchemaLocation="../robot_package/xml_schema/evaml_schema.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <settings>
    <voice type="pt_BR-faber-medium" pitchShift="5"/>
    <lightEffects mode="ON" />
    <audioEffects mode="ON" />
  </settings>
  <script>
    <light state="ON" color="WHITE"/>
    <wait duration="1000" />
    <light state="ON" color="BLUE"/>
    <wait duration="1000" />
    <light state="ON" color="GREEN"/>
    <wait duration="1000" />
    <light state="ON" color="PINK"/>
    <wait duration="1000" />
    <light state="ON" color="RED"/>
    <wait duration="1000" />
    <light state="ON" color="WHITE"/>
    <wait duration="1000" />
    <light state="ON" color="YELLOW"/>
    <wait duration="1000" />
  </script>
</evaml>